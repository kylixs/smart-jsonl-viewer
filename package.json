{
  "name": "jsonline-viewer",
  "version": "1.1.0",
  "description": "智能 JSONL 查看器 Chrome 扩展，支持字符串解码和高级过滤",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && npm run postbuild",
    "build:dev": "vue-tsc && vite build --mode development && npm run postbuild",
    "postbuild": "bash scripts/post-build.sh",
    "preview": "vite preview"
  },
  "keywords": [
    "jsonl",
    "viewer",
    "chrome-extension",
    "json",
    "decoder"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.1.32",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.3",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vue-tsc": "^3.2.1"
  },
  "dependencies": {
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "mermaid": "^11.12.2",
    "pinia": "^3.0.4",
    "shiki": "^3.20.0",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.8"
  }
}
